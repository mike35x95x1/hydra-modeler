{
  "name": "hydra-modeler",
  "version": "0.1.1",
  "description": "Database-agnostic, framework-independent, zero-dependency library for transforming flat datasets into nested JSON trees.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mike35x95x1/hydra-modeler.git"
  },
  "homepage": "https://github.com/mike35x95x1/hydra-modeler#readme",
  "bugs": {
    "url": "https://github.com/mike35x95x1/hydra-modeler/issues"
  },
  "license": "MIT",
  "author": "mike35x95x1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier . --check",
    "fmt:fix": "prettier . --write",
    "test": "jest --passWithNoTests"
  },
  "keywords": [
    "orm",
    "hydration",
    "models",
    "kysely",
    "sequelize",
    "typescript"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.0.0",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.44.0"
  }
}
